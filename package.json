{
    "name": "warden",
    "version": "1.0.0",
    "description": "Moderation bot built using the Anvil framework",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/app.js",
        "inspect": "npm run build && node --inspect dist/app.js",
        "inspect-break": "npm run build && node --inspect-brk dist/app.js",
        "postinstall": "npm run build",
        "local-copy-forge:linux": "sudo bash tasks/local-copy-forge.sh",
        "local-copy-forge:windows": "start tasks/local-copy-forge.bat"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/cloudrex/warden.git"
    },
    "author": "CloudRex <cloudrex@outlook.com>",
    "license": "MIT",
    "dependencies": {
        "@cloudrex/forge": "^1.2.4",
        "@types/knex": "^0.14.18",
        "@types/mongodb": "^3.1.4",
        "@types/node": "^10.9.4",
        "@types/string-similarity": "^1.2.0",
        "@types/table": "^4.0.5",
        "brain.js": "^1.2.8",
        "discord.js": "^11.4.2",
        "dotenv": "^6.0.0",
        "mongodb": "^3.1.4",
        "string-similarity": "^1.2.2",
        "table": "^5.0.2",
        "typescript": "^2.9.2"
    },
    "devDependencies": {
        "grunt": "^1.0.3"
    }
}
