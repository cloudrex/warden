    "dependencies": {
        "@cloudrex/forge": "^1.2.4",
        "@types/mongodb": "^3.1.4",
        "@types/node": "^11.13.0",
        "@types/string-similarity": "^1.2.0",
        "brain.js": "^1.2.8",
        "discord.js": "^11.4.2",
        "dotenv": "^6.0.0",
        "mongodb": "^3.1.4",
        "string-similarity": "^1.2.2",
        "typescript": "^2.9.2"
    }
