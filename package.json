{
    "name": "warden",
    "version": "1.0.0",
    "description": "Moderation bot built using the Mix framework",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/app.js",
        "inspect": "npm run build && node --inspect dist/app.js",
